        "# Now lets see what happens with a skewed probability\n",
        "# use p to set the bias\n",
        "def flip_biased(p):\n",
        "    return random.random() < p\n",
        "\n",
        "c1 = 0\n",
        "c2 = 0\n",
        "for i in range(1000):\n",
        "\n",
        "    if flip_biased(0.7):\n",
        "        c1 += 1\n",
        "    if flip_biased(0.25):\n",
        "        c2 += 1"
      ],
      "execution_count": null,
      "outputs": []
