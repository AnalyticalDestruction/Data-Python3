        "a = simple_gen()\n",
        "\n",
        "# use 'next' method to advance the iteration of the generator. has to be done at each step\n",
        "print(next(a))\n",
        "# use 'next' method to advance the iteration of the generator\n",
        "print(next(a))\n",
        "# use 'next' method to advance the iteration of the generator\n",
        "print(next(a))\n",
